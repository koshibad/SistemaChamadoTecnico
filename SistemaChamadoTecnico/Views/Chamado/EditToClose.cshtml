@model SistemaChamadoTecnico.Models.Chamado

@{
    ViewBag.Title = "EditToClose";
}

<h2>Finalizar</h2>

<h3>Tem certeza que deseja finalizar este Chamado?</h3>

<div>
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <h4>Chamado</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(model => model.TituloChamado)
        @Html.HiddenFor(model => model.DescricaoChamado)

        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.TituloChamado)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.TituloChamado)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.DescricaoChamado)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.DescricaoChamado)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.EstadoChamado)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.EstadoChamado)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.ObsevacaoChamado)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.ObsevacaoChamado)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.DataCriacaoChamado)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.DataCriacaoChamado)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.DataEncerramentoChamado)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.DataEncerramentoChamado)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Atendente.NomeAtendente)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Atendente.NomeAtendente)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Cliente.NomeCliente)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Cliente.NomeCliente)
            </dd>

        </dl>



        <div class="form-actions no-color">
            <input type="submit" value="Finalizar" class="btn btn-default" /> |
            @Html.ActionLink("Voltar", "List")
        </div>
    }
</div>
