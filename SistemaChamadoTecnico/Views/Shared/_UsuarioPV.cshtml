
@if (Request.IsAuthenticated)
{
    <ul class="nav navbar-nav navbar-right">
        <li>@Html.ActionLink(User.Identity.Name, "Index", "Home")</li>
        <li>@Html.ActionLink("Log Out", "Logout", "Autenticacao")</li>
    </ul>
}
else
{
    <ul class="nav navbar-nav navbar-right">
        <li>@Html.ActionLink("Cadastrar-se", "Create", "Autenticacao")</li>
        <li>@Html.ActionLink("Log In", "Login", "Autenticacao")</li>
    </ul>
}

